
#include <stdio.h>

int main()
{
    int a,b,c;
    printf("enter any three values\n");
    scanf("%d,%d,%d",&a,&b,&c);
    if ((a>b)&&(b>c))
    printf("the greatest number is %d,\n",a);
    else
    if ((b>c)&&(b>a))
    printf("the greatest number is %d\n",b);
    else
    printf("the greatest number is %d\n",c);
    return 0;
}
